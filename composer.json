{
  "name": "doblebjp/silex-user",
  "description": "User management for Silex and Doctrine ORM",
  "keywords": ["silex", "doctrine", "orm", "user"],
  "license": "MIT",
  "require": {
    "php": ">=5.4",
    "silex/silex": "~1.1",
    "doctrine/orm": "~2.4",
    "twig/twig": "~1.14",
    "symfony/doctrine-bridge": "~2.3",
    "symfony/twig-bridge": "~2.3",
    "symfony/security": "~2.3",
    "symfony/form": "~2.3",
    "symfony/yaml": "~2.3",
    "symfony/validator": "~2.3",
    "symfony/translation": "~2.3",
    "symfony/config": "~2.3",
    "knplabs/console-service-provider": "dev-master"
  },
  "require-dev": {
    "symfony/browser-kit": "~2.3",
    "symfony/css-selector": "~2.3"
  },
  "suggest": {
    "twbs/bootstrap": "~3.0"
  },
  "scripts": {
    "post-install-cmd": [
      "SilexUser\\Composer\\Callback::postInstall"
    ]
  },
  "bin": ["bin/silex-user"],
  "autoload": {
    "psr-0": {
      "SilexUser": "src"
    }
  }
}
